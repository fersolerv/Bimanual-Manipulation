#VRML V2.0 utf8
# A Cylinder to make a cover
  
Separator {  
    Transform {
        translation       0 0 0
        rotation          0 0 1  0
        #scaleFactor       1 1 1
        scaleOrientation  0 0 1  0
        center            0 0 0
    }
    Material {
            diffuseColor        1.0 0.7 0.4
            transparency 0.3
    }
    Cylinder {
	    height 30
	    radius 48
	    bottom TRUE
	    top TRUE
	    side TRUE
    }
}    
